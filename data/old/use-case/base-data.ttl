@base <http://base/> .
@prefix ecare: <http://www.example.org/ecare#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .

### Contexts ###

[] a ecare:Context ;
    ecare:forPerson <person1> ;
    ecare:forActivity ecare:Resting ;
    ecare:expectedHeartRate          [ ecare:lowerBound 65   ; ecare:upperBound 100  ] ;
    ecare:expectedBreathingRate      [ ecare:lowerBound 12   ; ecare:upperBound 25   ] ;
    ecare:expectedOxygenSaturation   [ ecare:lowerBound 92 ; ecare:upperBound 100 ] ;
    ecare:expectedTemperature        [ ecare:lowerBound 36.5 ; ecare:upperBound 37.5 ] ;
    rdfs:comment "Context describing Farida's expected physical parameter values when resting"@en .

[] a ecare:Context ;
    ecare:forPerson <person1> ;
    ecare:forActivity ecare:WatchingTV ;
    ecare:expectedHeartRate          [ ecare:lowerBound 80   ; ecare:upperBound 110  ] ;
    ecare:expectedBreathingRate      [ ecare:lowerBound 12   ; ecare:upperBound 30   ] ;
    ecare:expectedOxygenSaturation   [ ecare:lowerBound 92 ; ecare:upperBound 100 ] ;
    ecare:expectedTemperature        [ ecare:lowerBound 36.5 ; ecare:upperBound 37.5 ] ;
    rdfs:comment "Context describing Farida's expected physical parameter values when watching TV"@en .

[] a ecare:Context ;
    ecare:forPerson <person1> ;
    ecare:forActivity ecare:Exercising ;
    ecare:expectedHeartRate          [ ecare:lowerBound 70   ; ecare:upperBound 120  ] ;
    ecare:expectedBreathingRate      [ ecare:lowerBound 15   ; ecare:upperBound 40   ] ;
    ecare:expectedOxygenSaturation   [ ecare:lowerBound 92 ; ecare:upperBound 100 ] ;
    ecare:expectedTemperature        [ ecare:lowerBound 36.5 ; ecare:upperBound 38 ] ;
    rdfs:comment "Context describing Farida's expected physical parameter values when exercising"@en .

[] a ecare:Context ;
    ecare:forPerson <person1> ;
    ecare:forActivity ecare:Sleeping ;
    ecare:expectedHeartRate          [ ecare:lowerBound 60   ; ecare:upperBound 80  ] ;
    ecare:expectedBreathingRate      [ ecare:lowerBound 10   ; ecare:upperBound 20   ] ;
    ecare:expectedOxygenSaturation   [ ecare:lowerBound 92 ; ecare:upperBound 100 ] ;
    ecare:expectedTemperature        [ ecare:lowerBound 36.6 ; ecare:upperBound 37.6 ] ;
    rdfs:comment "Context describing Farida's expected physical parameter values when sleeping"@en .


### Persons ###

<person/1>
    a foaf:Person ;
    foaf:age 71 ;
    foaf:name "Farida" ;
    rdfs:comment "This resource represents Farida in the use-case scenario"@en .

### Observable properties ###

<person1/Activity>
    a sosa:ObservableProperty ;
    sosa:isObservedBy <activity/sensor1> ;
    ssn:isPropertyOf <person1> ;
    rdfs:label "Feature generated by the system representing Farida's current activity"@en .

<person1/HeartRate>
    a sosa:ObservableProperty ;
    sosa:isObservedBy <hr/sensor1> ;
    ssn:isPropertyOf <person1> ;
    rdfs:label "Measure of a Farida's heart rate"@en .

<person1/OxygenSaturation>
    a sosa:ObservableProperty ;
    sosa:isObservedBy <oxygen/sensor1> ;
    ssn:isPropertyOf <person1> ;
    rdfs:label "Measure of a Farida's oxygen saturation"@en .

<person1/BreathingRate>
    a sosa:ObservableProperty ;
    sosa:isObservedBy <breathing/sensor1> ;
    ssn:isPropertyOf <person1> ;
    rdfs:label "Measure of a Farida's breathing rate"@en .

<person1/Temperature>
    a sosa:ObservableProperty ;
    sosa:isObservedBy <temperature/sensor1> ;
    ssn:isPropertyOf <person1> ;
    rdfs:label "Measure of a Farida's temperature"@en .

### Sensors###

<activity/sensor1>
    a sosa:Sensor ;
    sosa:observes <person1/Activity> ;
    rdfs:comment "System derived observations reporting Farida's current activity" .

<hr/sensor1>
    a sosa:Sensor ;
    sosa:observes <person1/HeartRate> ;
    rdfs:comment "Sensor measuring Farida's heart rate" .

<oxygen/sensor1>
    a sosa:Sensor ;
    sosa:observes <person1/OxygenSaturation> ;
    rdfs:comment "Sensor measuring Farida's oxygen saturation" .

<breathing/sensor1>
    a sosa:Sensor ;
    sosa:observes <person1/BreathingRate> ;
    rdfs:comment "Sensor measuring Farida's breathing rate" .

<temperature/sensor1>
    a sosa:Sensor ;
    sosa:observes <person1/Temperature> ;
    rdfs:comment "Sensor measuring Farida's temperature" .
