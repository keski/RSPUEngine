BASE <http://base/>
PREFIX : <http://www.example.org/ontology#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX sosa: <http://www.w3.org/ns/sosa/>
PREFIX rspu: <http://w3id.org/rsp/rspu#>
PREFIX rspu-fn: <http://w3id.org/rsp/rspu/fn#>
PREFIX bn-ns: <http://w3id.org/rsp/rspu/bn#>

REGISTER STREAM <s> COMPUTED EVERY PT2S AS

SELECT DISTINCT *
FROM NAMED WINDOW <w> ON <http://ecareathome/stream/heartrate> [RANGE PT2S STEP PT2S]
WHERE {

   WINDOW <w> {
      GRAPH ?g {
          #?a a :HeartRate .
          << ?a sosa:hasSimpleResult ?hr >> rspu:error ?error .
          BIND(rspu-fn:add(rspu-fn:add(?error, 10), 10) AS ?d)
          BIND("1" AS ?t)
          BIND(?t AS ?p)
      }
   }
}